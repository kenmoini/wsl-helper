#!/bin/bash

case $ODO_VERSION in
    0.0.16|0.0.17|0.0.18|0.0.19|0.0.20)
        ;;
    0.0|0.0.*)
        ODO_VERSION=0.0.20
        ;;
    1.0|1.0.0|1.0.*)
        ODO_VERSION=1.0.0
        ;;
    1.2.6|1.2.*)
        ODO_VERSION=1.2.6
        ;;
    1.2.6|1.2.*)
        ODO_VERSION=1.2.6
        ;;
    2.0.0)
        ODO_VERSION=2.0.0
        ;;
    2.0.1)
        ODO_VERSION=2.0.1
        ;;
    2.0.2)
        ODO_VERSION=2.0.2
        ;;
    2.0.3)
        ODO_VERSION=2.0.3
        ;;
    *)
        ODO_VERSION=2.0.3
        ;;
esac

exec /usr/local/bin/odo-$ODO_VERSION "$@"